{
  "name": "codeups",
  "version": "1.0.0",
  "description": "MERN Stack Project: codeUps",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server2": "nodemon server.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\" \"npm run server2\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggharsha/codeUps.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ggharsha/codeUps/issues"
  },
  "homepage": "https://github.com/ggharsha/codeUps#readme",
  "dependencies": {
    "@babel/core": "^7.17.0",
    "@fontsource/inter": "^4.5.3",
    "aws-sdk": "^2.585.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.0",
    "multer": "^1.4.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "sass": "^1.49.7",
    "socket.io": "^4.4.1",
    "typewriter-effect": "^2.18.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
